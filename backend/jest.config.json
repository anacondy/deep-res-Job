{
  "testEnvironment": "node",
  "rootDir": "../",
  "modulePaths": [
    "<rootDir>/backend/node_modules"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ],
  "testMatch": [
    "<rootDir>/tests/**/*.test.js"
  ],
  "collectCoverageFrom": [
    "backend/**/*.js",
    "!backend/node_modules/**"
  ]
}
